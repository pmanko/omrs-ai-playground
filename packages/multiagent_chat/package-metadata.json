{
  "id": "multiagent-chat",
  "name": "Multi-Agent Medical Chat Application",
  "description": "A2A and MCP-based multi-agent chat application with medical expertise, integrating with OpenMRS FHIR and featuring semantic routing between specialized AI agents",
  "type": "infrastructure",
  "version": "0.1.0",
  "dependencies": [],
  "environmentVariables": {
    "MULTIAGENT_CHAT_SERVER_IMAGE": "multiagent-chat-server:latest",
    "MULTIAGENT_CHAT_CLIENT_IMAGE": "multiagent-chat-client:latest",
    "LLM_BASE_URL": "http://host.docker.internal:1234",
    "LLM_API_KEY": "",
    "GENERAL_MODEL": "gemma-3-12b-it",
    "MED_MODEL": "medgemma-4b-it-mlx",
    "LLM_TEMPERATURE": "0.2",
    "ORCHESTRATOR_PROVIDER": "openai",
    "ORCHESTRATOR_MODEL": "meta-llama-3.1-8b-instruct",
    "GEMINI_API_KEY": "",
    "ENABLE_A2A": "true",
    "ENABLE_A2A_NATIVE": "false",
    "CHAT_TIMEOUT_SECONDS": "90",
    "A2A_ROUTER_URL": "http://localhost:9100",
    "A2A_MEDGEMMA_URL": "http://localhost:9101",
    "A2A_CLINICAL_URL": "http://localhost:9102",
