    "OPENMRS_USERNAME": "admin",
    "OPENMRS_PASSWORD": "Admin123",
    "SPARK_THRIFT_HOST": "",
    "SPARK_THRIFT_PORT": "10000",
    "SPARK_THRIFT_DATABASE": "default",
    "SPARK_THRIFT_USER": "",
    "SPARK_THRIFT_PASSWORD": "",
    "FHIR_PARQUET_DIR": "",
    "MULTIAGENT_CHAT_SERVER_PORT": "3000",
    "MULTIAGENT_CHAT_CLIENT_PORT": "8091",
    "A2A_ROUTER_PORT": "9100",
    "A2A_MEDGEMMA_PORT": "9101",
    "A2A_CLINICAL_PORT": "9102"
  }
}